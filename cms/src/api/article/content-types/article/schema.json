{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "uid": {
      "type": "uid",
      "targetField": "title"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "architecture",
        "interior designer ",
        "feeling good ",
        "healing",
        "lifestyle + culture",
        "travel",
        "diy",
        "art"
      ]
    },
    "description": {
      "type": "text"
    },
    "authorName": {
      "type": "text"
    },
    "authorImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "coverImage": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "richContent": {
      "type": "blocks"
    },
    "similarArticles": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "article-blocks.text-block",
        "article-blocks.image-block"
      ]
    },
    "secondCategory": {
      "type": "enumeration",
      "enum": [
        "architecture",
        "interior designer ",
        "feeling good ",
        "healing",
        "lifestyle + culture",
        "travel",
        "diy",
        "art"
      ]
    }
  }
}
